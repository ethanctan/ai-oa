{
  "name": "ai-oa-extension",
  "displayName": "AI Technical Interviewer",
  "description": "An AI-powered interviewer for coding assessments that guides candidates through pre-interview, coding tasks, and post-interview review.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-oa.openChat",
        "title": "AI OA: Open Interviewer"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sidebar",
          "title": "AI Interviewer",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "sidebar": [
        {
          "id": "sidebar.myView",
          "name": "Interviewer"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "vscode:prepublish": "npm run compile",
    "compile": "echo 'No compilation needed for JavaScript'",
    "watch": "echo 'Watching not set up'"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^8.56.0"
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447",
  "dependencies": {
    "jszip": "^3.10.1"
  }
}
